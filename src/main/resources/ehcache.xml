<?xml version="1.0" encoding="UTF-8" ?>
<ehcache>
    <!--java.io.tmpdir 获取当前操作系统登陆用户的临时文件夹路径-->
    <diskStore path="java.io.tmpdir"/>
    <!--缓存设置  是否持续   缓存数量  存活时间 闲置时间 内存满了是否尝试放磁盘 -->
    <cache  name="user"
            maxElementsInMemory="100"
            eternal="false"
            timeToIdleSeconds="30"
            timeToLiveSeconds="30"
            overflowToDisk="true"

    />

</ehcache>